<Window x:Class="master.Windows.Blocks.IfWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:vm_condition="clr-namespace:master.ViewModels.Contract.Block.Conditioning"
        xmlns:vm_basetypes="clr-namespace:master.ViewModels.BaseTypes"
        xmlns:local="clr-namespace:master.Windows.Blocks"
        mc:Ignorable="d"
        Title="If config" Height="300" Width="300">
    <Window.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="../Views/DictionaryDisplays.xaml" />
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Window.Resources>

    <ContentControl Content="{Binding Condition}"/>

</Window>
